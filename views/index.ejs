<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>PRICIO</h1>
    <%amazon.forEach((item) => {%>
        <%=item.image %> <br>
        <%=item.link %> <br>
        <%=item.title %> <br>
        <%=item.price %> <br>
        <%=item.coupon %> <br>
        <%=item.shipping %> <br>
        <%=item.rating %> <br>
        <%=item.reviews %> <br>
    <%}) %>

    <hr>
    <hr>
    <%ebay.forEach((item) => {%>
        <%=item.image %> <br>
        <%=item.link %> <br>
        <%=item.title %> <br>
        <%=item.price %> <br>
        <%=item.discount %> <br>
        <%=item.shippingFrom %> <br>
        <%=item.shippingCost %> <br>
        <%=item.description %> <br>
        <%=item.hotness %> <br>
    <%}) %>
    
</body>
</html>